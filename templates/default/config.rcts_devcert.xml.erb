      <RMI>
        <RegistryPort>1090</RegistryPort>
        <ServicePorts from="" to=""/>
      </RMI>
      <application name="ETLBATCH" type="servicemanager">
        <property name="MonitoringAgentPort">
          <value>8091</value>
        </property>
        <services>
          <service name="JMSEtlProcessor">
            <property name="ThreadsCount">
              <value>20</value>
            </property>
            <property name="JMS.Queue.Package">
              <value>TM_ETL_PACKAGE</value>
            </property>
            <property name="JMS.Queue.Package.Failed">
              <value>TM_ETL_PACKAGE_FAILED</value>
            </property>
            <property name="JMS.Queue.Package.Invalid">
              <value>TM_ETL_PACKAGE_INVALID</value>
            </property>
            <property name="JMS.Queue.Package.DataIndexer">
              <value>TM_DI_PACKAGE</value>
            </property>
            <!-- Service will stop if the count of consecutive unknown errors reaches this threshold. -->
            <property name="ConsecutiveErrorsThreshold">
              <value>9999</value>
            </property>
            <!-- If not 0, then this is the maximum number of check resources attempts before the service is stopped. -->
            <property name="CheckResourcesMaxAttempts">
              <value>0</value>
            </property>
          </service>
          <service name="DataIndexerProcessor">
            <property name="ThreadsCount">
              <value>20</value>
            </property>
            <property name="JMS.Queue.DataIndexer">
              <value>TM_DI_PACKAGE</value>
            </property>
            <property name="JMS.Queue.DataIndexer.Failed">
              <value>TM_DI_PACKAGE_FAILED</value>
            </property>
            <property name="JMS.Queue.DataIndexer.Invalid">
              <value>TM_DI_PACKAGE_INVALID</value>
            </property>
            <!-- Service will stop if the count of consecutive unknown errors reaches this threshold. -->
            <property name="ConsecutiveErrorsThreshold">
              <value>9999</value>
            </property>
            <!-- If not 0, then this is the maximum number of check resources attempts before the service is stopped. -->
            <property name="CheckResourcesMaxAttempts">
              <value>0</value>
            </property>
          </service>
          <service name="ID Provider Service">
            <property name="JMXPort">
              <value>5678</value>
            </property>
          </service>
          <service name="XES Event Consumer Service">
            <property name="ThreadsCount">
              <value>20</value>
            </property>
            <property name="JMS.Queue.Event">
              <value>TM_XES_EVENT</value>
            </property>
            <property name="JMS.Queue.Event.Failed">
              <value>TM_XES_EVENT_FAILED</value>
            </property>
            <property name="JMS.Queue.Event.Invalid">
